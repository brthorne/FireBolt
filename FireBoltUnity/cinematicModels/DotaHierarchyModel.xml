<cinematicModel xmlns="http://liquidnarrative.csc.ncsu.edu/cinematicModel/v0.1" millisPerTick="33" domainDistancePerEngineDistance="80">
  <smartModelSettings maxActorSearchDepth="2" maxAnimationSearchDepth="2" />
  <terrain location="(-100,-2,-100)" terrainFileName="dota_terrain.prefab"/>
  <!-- animation search depth should generally be at least as deep as actor search depth-->
  <domainActions>
    <domainAction name="spawn-creep" >
      <params>
        <param name="actor" />
        <param name="position" />
      </params>
      <createActions>
        <createAction actorNameParamName="actor" originParamName="position" />
      </createActions>
      <destroyActions/>
      <rotateActions/>
      <translateActions/>
      <animateActions/>
    </domainAction>

    <domainAction name="spawn-hero" >
      <params>
        <param name="actor" />
        <param name="position" />
      </params>
      <createActions>
        <createAction actorNameParamName="actor" originParamName="position" />
      </createActions>
      <destroyActions/>
      <rotateActions/>
      <translateActions/>
      <animateActions/>
    </domainAction>

    <domainAction name="spawn-courier" >
      <params>
        <param name="actor" />
        <param name="position" />
      </params>
      <createActions>
        <createAction actorNameParamName="actor" originParamName="position" />
      </createActions>
      <destroyActions/>
      <rotateActions/>
      <translateActions/>
      <animateActions/>
    </domainAction>
    
    <domainAction name="move" >
      <params>
        <param name="actor" />
        <param name="destination" />
        <param name="origin" />
      </params>
      <createActions/>
      <destroyActions/>
      <translateActions>
        <translateAction  actorNameParamName="actor" destinationParamName="destination" originParamName="origin"/>
      </translateActions>
      <animateActions>
        <animateAction  name="walk" actorNameParamName="actor" effector="false" endingAction="idle"/>
      </animateActions>
      <rotateActions/>
    </domainAction>

   <domainAction name="rot" >
      <params>
        <param name="actor" />
      </params>
      <createActions> <!--can't locate the particle effect on pudge b/c we can't talk about relative locations in CM-->
      </createActions>
      <destroyActions/>
      <translateActions/>
      <animateActions>
        <animateAction name="rot" actorNameParamName="actor" effector="false" endingAction="idle"/>
      </animateActions>
      <rotateActions/>
    </domainAction>
    
    <domainAction name="creep-attack" >
      <params>
        <param name="actor" />
      </params>
      <createActions>
      </createActions>
      <destroyActions/>
      <translateActions/>
      <animateActions>
        <animateAction name="creep_attack" actorNameParamName="actor" effector="false" endingAction="idle"/>
      </animateActions>
      <rotateActions/>
    </domainAction>

    <domainAction name="damage" >
      <params>
        <param name="actor" />
        <param name="target" />
        <param name="source" />
        <param name="ability" />
        <param name="amount" />
        <param name="target-guessed" />
      </params>
      <createActions>
      </createActions>
      <destroyActions/>
      <translateActions/>
      <animateActions>
        <animateAction name="attack" actorNameParamName="source" effector="false" endingAction="idle"/>
      </animateActions>
      <rotateActions/>
    </domainAction>

    <domainAction name="rune-death" >
      <params>
        <param name="actor" />
      </params>
      <createActions/>
      <destroyActions>
        <destroyAction actorNameParamName="actor" />
      </destroyActions>
      <translateActions/>
      <animateActions/>        
      <rotateActions/>
    </domainAction>

    <domainAction name="meat-hook" >
      <params>
        <param name="actor" />
        <param name="target-position"/>
        <param name="target"/>
      </params>
      <createActions>
        <!--can't locate the hook on pudge b/c we can't talk about relative locations in CM-->
        <createAction actorNameParamName="hook" originParamName="target-position"/>
      </createActions>
      <destroyActions>
      </destroyActions>
      <translateActions/>
      <animateActions/>
      <rotateActions/>
    </domainAction>

    <domainAction name="die" >
      <params>
        <param name="actor" />
      </params>
      <createActions/>
      <rotateActions/>
      <translateActions/>
      <destroyActions>
        <destroyAction actorNameParamName="actor" effectorOffsetIndexName="onTheGround"/>
      </destroyActions>
      <animateActions>
        <animateAction name="die" actorNameParamName="actor" effector="true"/>
      </animateActions>
    </domainAction>
    
    <domainAction name="rotate">
      <params>
        <param name="actor" />
        <param name="end-angle" />
      </params>
      <createActions/>
      <destroyActions/>
      <translateActions/>
      <rotateActions>
        <rotateAction actorNameParamName="actor" destinationParamName="end-angle" maxDuration="400"/>
      </rotateActions>
      <animateActions/>
    </domainAction>   
  </domainActions>
  
  
