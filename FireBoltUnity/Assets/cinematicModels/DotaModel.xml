<cinematicModel millisPerTick="33" domainDistancePerEngineDistance="80">
  <domainActions>
    <domainAction name="spawn-creep" >
      <params>
        <param name="actor" />
        <param name="position" />
      </params>
      <createActions>
        <createAction actorNameParamName="actor" originParamName="position" />
      </createActions>
    </domainAction>
    <domainAction name="die" >
      <params>
        <param name="actor" />
      </params>
      <destroyActions>
        <destroyAction actorNameParamName="actor" effector="false" effectorOffsetIndexName="onTheGround"/>
      </destroyActions>
      <animateActions>
        <animateAction name="die" actorNameParamName="actor" effector="true"/>
      </animateActions>
    </domainAction>
		<domainAction name="buy-item" > 
      <params>
        <param name="actor" />
      </params>
      <animateActions>
        <animateAction name="buy-item1" actorNameParamName="actor"/>
      </animateActions>
    </domainAction>
    <domainAction name="doubleSwipe" >
      <params>
        <param name="actor1" />
        <param name="actor2" />                
      </params>
      <animateActions>
        <animateAction name="firstSwipe" actorNameParamName="actor1" effector="true"/>
        <animateAction name="secondSwipe" actorNameParamName="actor2" effectorOffsetIndexName="maxY"/>
      </animateActions>
    </domainAction>
    <domainAction name="enter" >
      <params>
        <param name="actor" />
        <param name="origin" />
      </params>
      <createActions>
        <createAction actorNameParamName="actor" originParamName="origin" />
      </createActions>
    </domainAction>
    <domainAction name="exit" >
      <params>
        <param name="actor" />
      </params>
      <createActions/>        
      <destroyActions>
        <destroyAction actorNameParamName="actor" />
      </destroyActions>
    </domainAction>
    <domainAction name="move" >
      <params>
        <param name="actor" />
        <param name="destination" />        
      </params>
      <createActions/>
      <destroyActions/>
      <translateActions>
        <translateAction  actorNameParamName="actor" destinationParamName="destination"/>
      </translateActions>
      <!--<rotateActions>
        <rotateAction  actorNameParamName="actor" destinationParamName="destination" maxDuration="400"/>
      </rotateActions>-->
      <animateActions>
        <animateAction  name="walk" actorNameParamName="actor" />    
      </animateActions>
    </domainAction>
    <domainAction name="rotate">
      <params>
        <param name="actor" />
        <param name="end-angle" />
      </params>
      <createActions/>
      <destroyActions/>
      <translateActions/>        
      <rotateActions>
        <rotateAction actorNameParamName="actor" destinationParamName="end-angle"/>
      </rotateActions>
      <animateActions/>
    </domainAction>
    <domainAction name="notSoSneaky">
      <params>
        <param name="actor" />
        <param name="observer" />
        <param name="destination" />
        <param name="spawnLocation" />
      </params>
      <createActions>
        <createAction actorNameParamName="observer" originParamName="spawnLocation" effectorOffsetIndexName="sneakAWhile"/>
      </createActions>
      <destroyActions/>
      <translateActions>
        <translateAction  actorNameParamName="actor" destinationParamName="destination"/>
      </translateActions>
      <rotateActions>
        <rotateAction  actorNameParamName="actor" destinationParamName="destination" maxDuration="700"/>
        <rotateAction  actorNameParamName="observer" destinationParamName="destination" maxDuration="800" effectorOffsetIndexName="sneakAWhile"/>
      </rotateActions>
      <animateActions>
        <animateAction name="sneak" actorNameParamName="actor" effector="true"/>
        <animateAction name="observe" actorNameParamName="observer" effector="false" effectorOffsetIndexName="sneakAWhile"/>
      </animateActions>
    </domainAction>
    <domainAction name="doNothing">
      <params>
        <param name="actor"/>
      </params>
      <animateActions>
        <animateAction name="standAround" actorNameParamName="actor"/>
      </animateActions>
    </domainAction>
  </domainActions>
