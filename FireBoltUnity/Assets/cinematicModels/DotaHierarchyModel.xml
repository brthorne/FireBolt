<cinematicModel millisPerTick="1" domainDistancePerEngineDistance="1">
<!---<cinematicModel millisPerTick="33" domainDistancePerEngineDistance="80">-->
  <smartModelSettings maxActorSearchDepth="2" maxAnimationSearchDepth="2" />
  <!-- animation search depth should generally be at least as deep as actor search depth-->
  <domainActions>
    <domainAction name="spawn-creep" >
      <params>
        <param name="actor" />
        <param name="position" />
      </params>
      <createActions>
        <createAction actorNameParamName="actor" originParamName="position" />
      </createActions>
    </domainAction>
    <domainAction name="die" >
      <params>
        <param name="actor" />
      </params>
      <destroyActions>
        <destroyAction actorNameParamName="actor" effector="false" effectorOffsetIndexName="onTheGround"/>
      </destroyActions>
      <animateActions>
        <animateAction name="die" actorNameParamName="actor" effector="true"/>
      </animateActions>
    </domainAction>
    <domainAction name="buy-item" >
      <params>
        <param name="actor" />
      </params>
      <animateActions>
        <animateAction name="buy-item1" actorNameParamName="actor"/>
      </animateActions>
    </domainAction>
    <domainAction name="idle" >
      <params>
        <param name="actor" />
      </params>
      <animateActions>
        <animateAction name="idle" actorNameParamName="actor"/>
      </animateActions>
    </domainAction>
    <domainAction name="spawn-hero" >
      <params>
        <param name="actor" />
        <param name="location" />
      </params>
      <createActions>
        <createAction actorNameParamName="actor" originParamName="location" />
      </createActions>
    </domainAction>
    <domainAction name="move" >
      <params>
        <param name="actor" />
        <param name="destination" />
        <param name="origin" />
      </params>
      <createActions/>
      <destroyActions/>
      <translateActions>
        <translateAction  actorNameParamName="actor" destinationParamName="destination" originParamName="origin"/>
      </translateActions>
      <animateActions>
        <animateAction  name="walk" actorNameParamName="actor" />
      </animateActions>
    </domainAction>
    <domainAction name="rotate">
      <params>
        <param name="actor" />
        <param name="end-angle" />
      </params>
      <createActions/>
      <destroyActions/>
      <translateActions/>
      <rotateActions>
        <rotateAction actorNameParamName="actor" destinationParamName="end-angle" maxDuration="400"/>
      </rotateActions>
      <animateActions/>
    </domainAction>
    <domainAction name="notSoSneaky">
      <params>
        <param name="actor" />
        <param name="observer" />
        <param name="destination" />
        <param name="spawnLocation" />
      </params>
      <createActions>
        <createAction actorNameParamName="observer" originParamName="spawnLocation" effectorOffsetIndexName="sneakAWhile"/>
      </createActions>
      <destroyActions/>
      <translateActions>
        <translateAction  actorNameParamName="actor" destinationParamName="destination"/>
      </translateActions>
      <rotateActions>
        <rotateAction  actorNameParamName="actor" destinationParamName="destination" maxDuration="700"/>
        <rotateAction  actorNameParamName="observer" destinationParamName="destination" maxDuration="800" effectorOffsetIndexName="sneakAWhile"/>
      </rotateActions>
      <animateActions>
        <animateAction name="sneak" actorNameParamName="actor" effector="true"/>
        <animateAction name="observe" actorNameParamName="observer" effector="false" effectorOffsetIndexName="sneakAWhile"/>
      </animateActions>
    </domainAction>
  </domainActions>
  <actors>
    <actor name="secretAgentMan" model="m005.fbx" >
      <animationMappings>
        <animationMapping animateActionName="observe" animationName="idle" loopAnimation="true" />
        <animationMapping animateActionName="walk" animationName="cm_walk" loopAnimation="true" />
      </animationMappings>
    </actor>
    <actor name="Pudge" model="Heroes/pudge/pudge_model.fbx" >
      <animationMappings>
        <animationMapping animateActionName="sneak" animationName="sneak" loopAnimation="true" />
        <animationMapping animateActionName="walk" animationName="walk" loopAnimation="true" />
        <animationMapping animateActionName="buy-item1" animationName="verticalSwipe" loopAnimation="true" />
        <animationMapping animateActionName="idle" animationName="idle" loopAnimation="true" />
      </animationMappings>
    </actor>


    <actor name="Mirana" model="Heroes/mirana/mirana_model.fbx" >
